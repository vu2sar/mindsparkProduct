@font-face 
{
	font-family:"Comic Sans MS";
	src:url("../../../assets/comic.ttf") format("truetype");
}
@charset "utf-8";
*{
	font-family:Comic Sans MS;
}
html,body,div,p,span,canvas{
	margin:0;
	padding:0;
}
table
{
	border-collapse:collapse;
}
th
{
	width:50px;
	background-color:#f4e48a;
	border:1px solid red;
}
td
{
	background-color:#fbf4ce;
	border:1px solid red;
	text-align: center;
	height:27px;
}
.no
{
	margin-left:10px;
	width:200px;
	margin-left:10px;
}
body
{	
	width: 800px;
	height: 600px;
	background-repeat:no-repeat;
	font-family:"Comic Sans MS";
	font-size:16px;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-o-user-select: none;
	user-select: none; 
	overflow:hidden;
}
.fraction 
{
  display: inline-block;
  text-align: center;
  vertical-align: middle;
  margin-left: 2px;
  margin-right: 2px;
}
.frac
{
  line-height: 100%;
  padding-left: 6px;
  padding-right: 6px;
}
.numerator 
{
  border-bottom: 1px solid;
}
#numbersDiv
{
	padding-left:30px;
	padding-top:45px;
	width:300px;
	height:400px;
}
.prompt
{
	position: absolute;
	border:1px solid #048593;
	border-radius:10px;
	background-color:#c2f7fe;
	cursor: pointer;
}
#fillDiv
{
	left:400px;
	top:150px;
	width:350px;
	height:75px;
}
#correctDiv
{
	left:500px;
	top:50px;
	width:125px;
	height:100px;
}
#wrongDiv1
{
	left:500px;
	top:225px;
	width:225px;
	height:110px;
}
#wrongDiv2
{
	left:500px;
	top:225px;
	width:225px;
	height:135px;
}
#expDiv
{
	left:375px;
	top:325px;
	width:350px;
	height:225px;
}
.ans
{
	width:75px;
	border:1px solid black;
	text-align: center;
}
.button
{
	cursor: pointer;
}
input
{
	font-family:"Comic Sans MS";
	font-size:16px;
}
#container
{
	width: 800px;
	height:600px;
	background-image: url('../assets/background.png');
	overflow:hidden;
	-webkit-transform:scale(1);
	-webkit-transform-origin : left top;
	-moz-transform :scale(1);
	-moz-transform-origin : left top;
	-ms-transform :scale(1);
	-ms-transform-origin : left top;
	-o-transform :scale(1);
	-o-transform-origin : left top;
	transform :scale(1);
	transform-origin : left top;
	
}
.img
{
	height:44px;
	width:36px;
}
.arrowImg, #arrow
{
	height:15px;
	width:10px;
}
#arrow
{
	position: absolute;
	left:95px;
	top:100px;
}

input[type="text"] {
    -webkit-appearance: none;
}